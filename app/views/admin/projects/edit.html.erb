<%= link_to 'Home', root_path %>/
<%= link_to 'Admin', admin_path %>/
<%= link_to 'Projet', admin_projects_path %>/
<%= link_to 'Editer', edit_admin_project_path(@project.id) %>

<h1>Editer un projet:</h1>

<%= form_for @project, url: admin_project_path(@project.id) do |f| %>

	<%= render 'fields', f: f %>

	<table><tr><td>Editer le projet</td><td><%= f.submit "Enregistrer les modifications", class: 'w8-button l-blue' %></td></tr></table>

<% end %>